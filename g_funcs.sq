function angle2dir( degree ) {
	degree = ::SimplifyDegrees( degree )
	if ( degree < 45 ) {
		return ::NORTH
	}
	if ( degree < 90 ) {
		return ::NORTHEAST
	}
	if ( degree < 135 ) {
		return ::EAST
	}
	if ( degree < 180 ) {
		return ::SOUTHEAST
	}
	if ( degree < 225 ) {
		return ::SOUTH
	}
	if ( degree < 270 ) {
		return ::SOUTHWEST
	}
	if ( degree < 315 ) {
		return ::WEST
	}
	return 9
}

function angle2text( degree ) {
	return ::dir2text( ::angle2dir( degree ) )
}

