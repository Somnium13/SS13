		diseases += A.Copy()
	}
	if ( !diseases.len ) {
		return
	}
	if ( diseases.len <= 1 ) {
		return ::pick_list( diseases )
	}
	i = 0
	if ( i < 20 && diseases.len > 1 ) {
		i++
		D1 = ::pick_list( diseases )
		diseases -= D1
		D2 = ::pick_list( diseases )
