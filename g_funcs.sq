			while (j <= ::rand( 0, 2 )) {
				local _ = {PICK_RANDOM 0 - 65535} // Was a pick, sorry for the mess.
				if ( _ < 3855 ) { _ = "#" }
				else if ( _ < 7710 ) { _ = "@" }
				else if ( _ < 11565 ) { _ = "*" }
				else if ( _ < 15420 ) { _ = "&" }
				else if ( _ < 19275 ) { _ = "%" }
				else if ( _ < 23130 ) { _ = "$" }
				else if ( _ < 26985 ) { _ = "/" }
				else if ( _ < 30840 ) { _ = "<" }
				else if ( _ < 34695 ) { _ = ">" }
				else if ( _ < 38550 ) { _ = ";" }
				else if ( _ < 42405 ) { _ = "*" }
				else if ( _ < 46260 ) { _ = "*" }
				else if ( _ < 50115 ) { _ = "*" }
				else if ( _ < 53970 ) { _ = "*" }
				else if ( _ < 57825 ) { _ = "*" }
				else if ( _ < 61680 ) { _ = "*" }
				else  { _ = "*" }

