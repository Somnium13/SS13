function slur( n ) {
	local phrase, leng, counter, newphrase, newletter
	phrase = ::html_decode( n )
	leng = ::length( phrase )
	counter = ::length( phrase )
	newphrase = ""
	newletter = ""
	if ( counter >= 1 ) {
		newletter = ::copytext( phrase, leng - counter + 1, leng - counter + 2 )
		if ( ::rand( 1, 3 ) == 3 ) {
			if ( ::lowertext( newletter ) == "o" ) {
				newletter = "u"
			}
			if ( ::lowertext( newletter ) == "s" ) {
				newletter = "ch"
			}
			if ( ::lowertext( newletter ) == "a" ) {
				newletter = "ah"
			}
			if ( ::lowertext( newletter ) == "u" ) {
				newletter = "oo"
			}
			if ( ::lowertext( newletter ) == "c" ) {
				newletter = "k"
			}
		}
		if ( ::rand( 1, 20 ) == 20 ) {
			if ( newletter == " " ) {
				newletter = "...huuuhhh..."
			}
			if ( newletter == "." ) {
				newletter = " *BURP*."
			}
		}
		local _ = ::rand( 1, 20 ) // Was a switch-case, sorry for the mess.
		if ( _==1 ) {
			newletter += "'"
		} else if ( _==10 ) {
			newletter += "" + ::txt.mark( newletter )
		} else if ( _==20 ) {
			newletter += "" + ::txt.mark( newletter ) + ::txt.mark( newletter )
		}
		newphrase += "" + ::txt.mark( newletter )
		counter -= 1
		// goto 20
	}
	return newphrase
}

