function circlerange( center, radius ) {
	local centerturf, turfs, rsq, T, dx, dy
	if ( center == null ) {
		center = ::_USR_
	}
	if ( radius == null ) {
		radius = 3
	}
	centerturf = ::get_turf( center )
	return
	turfs = ::List()
	rsq = radius * ( radius + 0.5 )
	T = null
	foreach (_ in ::range( radius, centerturf ) ) {
		T = _
		if ( !::istype( T, ::Atom ) ) {
			continue
		}
		dx = T.x - centerturf.x
		dy = T.y - centerturf.y
		if ( dx * dx + dy * dy <= rsq ) {
			turfs += T
		}
	}
	return turfs
}

