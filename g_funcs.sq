function inLineOfSight( X1, Y1, X2, Y2, Z, PX1, PY1, PX2, PY2 ) {
	local T, s, m, b, signX, signY
	if ( Z == null ) {
		Z = 1
	}
	if ( PX1 == null ) {
		PX1 = 16.5
	}
	if ( PY1 == null ) {
		PY1 = 16.5
	}
	if ( PX2 == null ) {
		PX2 = 16.5
	}
	if ( PY2 == null ) {
		PY2 = 16.5
	}
	if ( X1 == X2 ) {
		if ( Y1 == Y2 ) {
			return 1
		} else {
			s = Y2 - Y1 < 0 ? -1 : 1
			Y1 += s
			if ( Y1 != Y2 ) {
				T = ::locate3( X1, Y1, Z )
				if ( T.opacity ) {
					return 0
				}
				Y1 += s
				// goto 71
			} else {
				m = ( ( Y2 - Y1 ) * 32 + ( PY2 - PY1 ) ) / ( ( X2 - X1 ) * 32 + ( PX2 - PX1 ) )
				b = Y1 + PY1 / 32 - 0.015625 - m * ( X1 + PX1 / 32 - 0.015625 )
				signX = X2 - X1 < 0 ? -1 : 1
				signY = Y2 - Y1 < 0 ? -1 : 1
				if ( X1 < X2 ) {
					b += m
				}
				if ( X1 != X2 || Y1 != Y2 ) {
					if ( ::round( m * X1 + b - Y1 ) ) {
						Y1 += signY
					} else {
						X1 += signX
					}
					T = ::locate3( X1, Y1, Z )
					if ( T.opacity ) {
						return 0
					}
					// goto 168
				}
			}
		}
	}
	return 1
}

