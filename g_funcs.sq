function trim_left( text ) {
	local i
	i = null
	foreach (_ in ::gen_range( 1, ::length( text ) ) ) {
		i = _
		if ( ::text2ascii( text, i ) > 32 ) {
			return ::copytext( text, i, null )
		}
	}
	return ""
}

function trim_right( text ) {
	local i
	i = null
	i = ::length( text )
	while (i > 0) {
		if ( ::text2ascii( text, i ) > 32 ) {
			return ::copytext( text, 1, i + 1 )
		}
		i--
	}
	return ""
}

