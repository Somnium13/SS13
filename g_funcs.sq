function Get_Angle( start, end ) {
	local dy, dx, _default
	if ( !start || !end ) {
		return 0
	}
	dy = end.y * 32 + end.pixel_y - ( start.y * 32 + start.pixel_y )
	dx = end.x * 32 + end.pixel_x - ( start.x * 32 + start.pixel_x )
	if ( !dy ) {
		return dx >= 0 ? 90 : 270
	}
	_default = ::arctan( dx / dy )
	if ( dy < 0 ) {
		_default += 180
	} else if ( dx < 0 ) {
		_default += 360
	}
	return _default
}

function get_area( O ) {
